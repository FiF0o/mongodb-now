{
  "name": "mongodb-now-deploy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "//": "Runs now-start instead of start when deployed with now",
    "now-start": "node ./bin/www",
    "//": "use now --static to force deploying/serving static assets",
    "serve-static": "cd public && ./node_modules/.bin/now"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "dotenv": "4.0.0",
    "now": "4.2.2"
  }
}
